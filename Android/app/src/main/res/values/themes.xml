<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.Sample" parent="Theme.Felwal">
        <item name="android:statusBarColor" tools:targetApi="l">?android:colorBackground</item>

        <!-- action bar -->
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">false</item>

        <!-- Primary brand color -->
        <item name="colorPrimary">@color/material_purple_500</item>
        <item name="colorPrimaryVariant">@color/material_purple_700</item>
        <item name="colorOnPrimary">@color/white</item>

        <!-- Secondary brand color -->
        <item name="colorSecondary">@color/material_teal_200</item>
        <item name="colorSecondaryVariant">@color/material_teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
    </style>

</resources>